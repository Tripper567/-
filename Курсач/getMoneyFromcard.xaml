<Window x:Class="Курсач.getMoneyFromcard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсач"
        mc:Ignorable="d"
        Title="Снять деньги с карты" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="cardsBC" HorizontalAlignment="Left" Margin="45,91,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="BalanceCB" HorizontalAlignment="Left" Height="23" Margin="45,159,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Снять" HorizontalAlignment="Left" Margin="45,212,0,0" VerticalAlignment="Top" Width="82" Click="Button_Click" Height="33"/>
        <Label Content="Снятие " HorizontalAlignment="Left" Margin="105,10,0,0" VerticalAlignment="Top" FontSize="22"/>
        <Label Content="Выберите карту :" HorizontalAlignment="Left" Margin="45,54,0,0" VerticalAlignment="Top"/>
        <Label Content=" Введите сумму :" HorizontalAlignment="Left" Margin="45,122,0,0" VerticalAlignment="Top"/>
        <Button Content="Назад" HorizontalAlignment="Left" Margin="173,212,0,0" VerticalAlignment="Top" Width="82" Height="33" Click="Button_Click_1"/>

    </Grid>
</Window>
